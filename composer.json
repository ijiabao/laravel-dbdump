{
    "name": "ijiabao/laravel-dbdump",
	"type": "package",
    "description": "laravel database export/import manager",
	"keywords": ["laravel dbdump", "laravel database", "db export", "db import", "db dump", "ijiabao"],
    "license": "MIT",
    "authors": [
        {
            "name": "ijiabao",
            "email": "ijiabao@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
		"php": ">=5.4",
		"illuminate/config": "~5.0",
		"illuminate/console": "~5.0",
		"illuminate/support": "~5.0"
	},
    "autoload": {
        "psr-4": {
            "iJiabao\\DbDump\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iJiabao\\DbDump\\DbDumpServiceProvider"
            ],
            "aliases": {
                "DbDump": "iJiabao\\DbDump\\Facades\\DbDump"
            }
        }
    }

}
